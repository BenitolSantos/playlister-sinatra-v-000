<h1>genres_name</h1>
<br>
<h1><%=@genre.name%></h1>
<br>
<h1>All songs of this genre</h1>
<br>
<%@genre.songs.each_with_index do |song,index|%>
<h1>song_<%=index+1%>_name</h1><a href="/songs/<%=song.slug%>"><%= song.name %></a>
<%end%>
<h1>All artists of this genre</h1>
<br>
<%@genre.artists.each_with_index do |artist,index|%>
<h1>genre_<%=index+1%>_name</h1><a href="/artist/<%=artist.slug%>"><%= artist.name %></a>
<%end%>
